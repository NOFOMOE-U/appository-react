{
  "name": "appository",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "NOTE: make sure to add {appname} --prod":"when using build",
    "test": "nx test",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:components": "nx affected:components",
    "affected:build": "nx affected:build",
    "shared-storyui": "NODE_OPTIONS='--openssl-legacy-provider' nx run shared-ui:storybook",
    "build-storyui": "NODE_OPTIONS='--openssl-legacy-provider' nx run shared-ui:build-storybook",
    "story": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6006",
    "build-story": "NODE_OPTIONS='--openssl-legacy-provider'  build-storybook",
    "serveall": "nx run-many --parallel --target=serve --projects=host,dashboard,login,admin,bookstore --maxParallel=100",
    "api": "nx serve backend-api",
    "graph": "nx dep-graph",
    "affected:graph:build": "nx affected:graph:build",
    "affected:graph:test": "nx affected:graph:test",
    "affected:graph": "nx affected:dep-graph",
    "preinstall": "npx only-allow pnpm",
    "docker1": "commands",
    "dockers:dev": "docker-compose -f tools/nofomoe-environment/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "prisma2": "commands",
    "generate": "prisma generate --schema ./libs/backend/data-access/src/lib/prisma/schema.prisma",
    "migrate-save": "prisma migrate dev --create-only --preview-feature --schema ./libs/backend/data-access/src/lib/prisma/schema.prisma",
    "migrate-up": "prisma migrate dev --preview-feature --schema ./libs/backend/data-access/src/lib/prisma/schema.prisma",
    "migrate-reset": "npx prisma migrate reset --preview-feature --schema ./libs/backend/data-access/src/lib//prisma/schema.prisma",
    "migrate-deploy": "npx prisma migrate deploy --preview-feature --schema ./libs/backend/data-access/src/lib//prisma/schema.prisma",
    "migrate-status": "npx prisma migrate status --preview-feature --schema ./libs/backend/data-access/src/lib/prisma/schema.prisma",
    "db-pull": "prisma db pull --schema ./libs/backend/data-access/prisma/schema.prisma",
    "db:migrate:dev": "npx env-cmd -f .local.env npx prisma migrate dev",
    "db:studio": "npx env-cmd -f .local.env npx prisma studio"
  },
