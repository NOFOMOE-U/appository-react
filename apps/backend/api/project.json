{
  "name": "backend-api",
  "$schema": "../../../node_modules/nx/schemas/nx-schema.json",
  "sourceRoot": "apps/backend/api/src",
  "projectType": "application",
  "prefix": "api",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/backend/api",
        "main": "apps/backend/api/src/main.ts",
        "tsConfig": "apps/backend/api/tsconfig.app.json",
        "assets": [
          "apps/backend/api/src/assets"
        ],
        "webpackConfig": "apps/backend/api/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "backend-api:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "backend-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/backend/api/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/backend/api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "data-model": {
    "root": "libs/backend/data-model",
    "sourceRoot": "libs/backend/data-model/src",
    "projectType": "library",
    "targets": {
      "lint": {
        "executor": "@nrwl/linter:eslint",
        "options": {
          "lintFilePatterns": [
            "libs/backend/data-model/**/*.ts"
          ]
        }
      },
      "test": {
        "executor": "@nrwl/jest:jest",
        "outputs": [
          "coverage/libs/backend/data-model"
        ],
        "options": {
          "passWithNoTests": true
        }
      },
      "migrate-up": {
        "executor": "nx-plugin-workspace:exec",
        "outputs": [],
        "options": {
          "command": "migrate dev --preview-feature --schema ./libs/backend/data-model/schema/schema.prisma",
          "cwd": "libs/backend/data-model"
        }
      },
      "migrate-reset": {
        "executor": "@nrwl/jest:jest",
        "outputs": [],
        "options": {
          "command": "npx prisma migrate reset --preview-feature --schema ./libs/backend/data-model/schema/schema.prisma",
          "cwd": "libs/backend/data-model"
        }
      },
      "migrate-deploy": {
        "executor": "@nrwl/jest:jest",
        "outputs": [],
        "options": {
          "command": "npx prisma migrate deploy --preview-feature --schema ./libs/backend/data-model/schema/schema.prisma",
          "cwd": "libs/backend/data-model"
        }
      },
      "migrate-status": {
        "executor": "@nrwl/jest:jest",
        "outputs": [],
        "options": {
          "command": "npx prisma migrate status --preview-feature --schema ./libs/backend/data-model/schema/schema.prisma",
          "cwd": "libs/data-model"
        }
      }
    }
  },
  "tags": []
}